<?xml version="1.0" encoding="utf-8"?>
<Map background-color='#c7bc7b' srs="+init=epsg:3857">
  <Style name ="roads1">
    <Rule>
    <LineSymbolizer stroke="#525252" stroke-width="2"/>
    </Rule>
  </Style>

  <Style name ="roads2">
    <Rule>
    <Filter>([fclass]='secondary')</Filter>
    <LineSymbolizer stroke="#f5b042" stroke-width="6"/>
    </Rule>
  </Style>

  <Style name ="roads3">
    <Rule>
    <Filter>([fclass]='primary')</Filter>
    <LineSymbolizer stroke="#fcdb47" stroke-width="8"/>
    </Rule>
  </Style>

  <Style name ="roads_text">
    <Rule>
    <Filter>([fclass]='primary' or [fclass]='secondary')</Filter>
    <TextSymbolizer placement ="line" face-name="DejaVu Sans Book" size ="8" fill="black" halo-fill="white" halo-radius="1">[name] </TextSymbolizer>
    </Rule>
  </Style>

  <Style name ="buildings">
    <Rule>
    <PolygonSymbolizer fill ="#b01a95"/>
    </Rule>
  </Style>

  <Style name ="pois">
    <Rule>
    <TextSymbolizer placement ="point" face-name="DejaVu Sans Book" size ="8" fill="black" halo-fill="white" halo-radius="1">[name] </TextSymbolizer>
    </Rule>
  </Style>

  <Style name="river">
    <Rule>
      <LineSymbolizer stroke="#1470ba" stroke-width="6"/>
    </Rule>
  </Style>

  <Style name="water">
    <Rule>
      <PolygonSymbolizer fill="#1470ba"/>
    </Rule>
  </Style>

  <Style name="landuse">
    <Rule>
      <Filter>[fclass]='forest' or [fclass]='park' or [fclass]='cemetery' or [fclass]='meadow' or [fclass]='nature_reserve'</Filter>
      <PolygonSymbolizer fill="#97fc72"/>
    </Rule>
  </Style>

